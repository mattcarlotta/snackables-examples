{
  "name": "snackables-examples",
  "version": "0.0.1",
  "description": "Loading byte sized ENVs made simple.",
  "homepage": "https://github.com/mattcarlotta/snackables-example",
  "scripts": {
    "ex:arrpaths": "ts-node examples/arrpaths.ts",
    "ex:assign": "ts-node examples/assign.ts",
    "ex:basic": "ENV_DIR=examples ENV_LOAD=.env.test ENV_DEBUG=true ts-node examples/basic.ts",
    "ex:config": "ts-node examples/config.ts",
    "ex:commandline": "ENV_DIR=examples ENV_LOAD=.env.command ENV_DEBUG=true ts-node examples/commandline.ts",
    "ex:interpolate": "ts-node examples/interpolate.ts",
    "ex:load": "ts-node examples/load.ts",
    "ex:loadconfig": "LOAD_CONFIG=test ts-node -r snackables examples/loadconfig.ts",
    "ex:parse": "ts-node examples/parse.ts",
    "ex:paths": "ts-node examples/paths.ts",
    "ex:preload": "ENV_LOAD=.env ENV_DEBUG=true ts-node -r snackables examples/preload.ts",
    "ex:preloadcustompaths": "ENV_LOAD=customdir/.env.base,customdir/.env.test ENV_DEBUG=true ts-node -r snackables examples/preloadcustompaths.ts",
    "ex:preloaddir": "ENV_DIR=examples ENV_LOAD=.env.base,.env.test ENV_DEBUG=true ts-node -r snackables examples/preloaddir.ts",
    "ex:override": "ts-node examples/override.ts",
    "ex:strpaths": "ts-node examples/strpaths.ts",
    "lint": "eslint ./ --ext .ts,.js"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:mattcarlotta/snackables-next.git"
  },
  "keywords": [
    "snackables",
    "env",
    ".env",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "author": "Matt Carlotta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattcarlotta/snackables-examples/issues"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "browserslist": "^4.16.6",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.0",
    "snackables": "^3.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {}
}
